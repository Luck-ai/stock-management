# Copy this file to .env and fill in real values. Do NOT commit secrets.

# Database (asyncpg URL)
# Example: postgresql+asyncpg://<user>:<password>@localhost:5432/<dbname>
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/stockdb

# SendGrid API key (if using email features)
SENDGRID_API_KEY=SG.YOUR_SENDGRID_KEY_HERE

# JWT secret used to sign access tokens. Use a strong random value in production.
JWT_SECRET=your_jwt_secret_here

# Access token expiry in minutes (optional)
ACCESS_TOKEN_EXPIRE_MINUTES=60
